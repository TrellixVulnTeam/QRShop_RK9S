n              </div>\r\n              <form role=\"form\">\r\n                <baseinput\r\n                  alternative\r\n                  class=\"mb-3\"\r\n                  placeholder=\"Email\"\r\n                  addonLeftIcon=\"glyphicon glyphicon-envelope\"\r\n                >\r\n                </baseinput>\r\n                <baseinput\r\n                  alternative\r\n                  type=\"password\"\r\n                  placeholder=\"Password\"\r\n                  addonLeftIcon=\"ni ni-lock-circle-open\"\r\n                >\r\n                </baseinput>\r\n                <base-checkbox> Remember me </base-checkbox>\r\n                <div class=\"text-center\">\r\n                  <basebutton type=\"primary\" v-on:click=\"login\" class=\"my-4\"\r\n                    >Sign In</basebutton\r\n                  >\r\n                </div>\r\n              </form>\r\n            </template>\r\n          </card>\r\n          <div class=\"row mt-3\">\r\n            <div class=\"col-6\">\r\n              <a href=\"#\" class=\"text-light\">\r\n                <small>Forgot password?</small>\r\n              </a>\r\n            </div>\r\n            <div class=\"col-6 text-right\">\r\n              <a href=\"#\" class=\"text-light\">\r\n                <small>Create new account</small>\r\n              </a>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </section>\r\n</template>\r\n<script>\r\nimport card from \"../components/miniComponents/Card.vue\";\r\nimport basebutton from \"../components/miniComponents/BaseButton.vue\";\r\nimport baseinput from \"../components/miniComponents/BaseInput.vue\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"login\",\r\n  components: {\r\n    card: card,\r\n    basebutton: basebutton,\r\n    baseinput: baseinput,\r\n  },\r\n  methods: {\r\n    login() {\r\n      axios({\r\n        method: \"post\",\r\n        url: \"/api/login\",\r\n        data: {\r\n          email: \"root@gmail.com\",\r\n          password: \"000\",\r\n        },\r\n      }).then(\r\n        (response) => {\r\n          localStorage.setItem(\"token\", response.data.accessToken);\r\n          axios.defaults.headers.common[\"x-access-token\"] =\r\n            response.data.accessToken;\r\n          return response;\r\n        },\r\n        (error) => {\r\n          return error;\r\n        }\r\n      );\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.section-shaped {\r\n  margin: 180px 0;\r\n  position: relative;\r\n}\r\n.section-shaped .shape {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  z-index: -1;\r\n  width: 100%;\r\n  height: 100%;\r\n  overflow: hidden;\r\n}\r\n.section {\r\n  padding-top: 4rem;\r\n  padding-bottom: 4rem;\r\n}\r\n.section-shaped .shape + .container {\r\n  position: relative;\r\n  padding-top: 6rem;\r\n  padding-bottom: 0;\r\n  height: 100%;\r\n}\r\n.bg-gradient-default {\r\n  background: linear-gradient(87deg, #172b4d, #1a174d) !important;\r\n}\r\n.shape span {\r\n  position: absolute;\r\n  list-style: none;\r\n  display: block;\r\n  width: 40px;\r\n  height: 40px;\r\n  background: hsla(0, 0%, 100%, 0.1);\r\n  bottom: -160px;\r\n  -webkit-animation: square 25s infinite;\r\n  animation: square 25s infinite;\r\n  -webkit-transition-timing-function: linear;\r\n  transition-timing-function: linear;\r\n  &:nth-child(1) {\r\n    left: 10%;\r\n  }\r\n  &:nth-child(2) {\r\n    left: 20%;\r\n    width: 80px;\r\n    height: 80px;\r\n    animation-delay: 2s;\r\n    animation-duration: 17s;\r\n  }\r\n  &:nth-child(3) {\r\n    left: 25%;\r\n    animation-delay: 4s;\r\n  }\r\n  &:nth-child(4) {\r\n    left: 40%;\r\n    width: 60px;\r\n    height: 60px;\r\n    animation-duration: 22s;\r\n    background-color: fade(white, 25%);\r\n  }\r\n  &:nth-child(5) {\r\n    left: 70%;\r\n  }\r\n  &:nth-child(6) {\r\n    left: 80%;\r\n    width: 120px;\r\n    height: 120px;\r\n    animation-delay: 3s;\r\n    background-color: fade(white, 20%);\r\n  }\r\n  &:nth-child(7) {\r\n    left: -2%;\r\n    width: 160px;\r\n    height: 160px;\r\n    animation-delay: 7s;\r\n  }\r\n  &:nth-child(8) {\r\n    left: 55%;\r\n    width: 20px;\r\n    height: 20px;\r\n    animation-delay: 15s;\r\n    animation-duration: 40s;\r\n  }\r\n  &:nth-child(9) {\r\n    left: 25%;\r\n    width: 10px;\r\n    height: 10px;\r\n    animation-delay: 2s;\r\n    animation-duration: 40s;\r\n    background-color: fade(white, 30%);\r\n  }\r\n  &:nth-child(10) {\r\n    left: 90%;\r\n    width: 160px;\r\n    height: 160px;\r\n    animation-delay: 11s;\r\n  }\r\n\r\n  @-webkit-keyframes square {\r\n    0% {\r\n      transform: translateY(0);\r\n    }\r\n    100% {\r\n      transform: translateY(-700px) rotate(600deg);\r\n    }\r\n  }\r\n  @keyframes square {\r\n    0% {\r\n      transform: translateY(0);\r\n    }\r\n    100% {\r\n      transform: translateY(-700px) rotate(600deg);\r\n    }\r\n  }\r\n}\r\n</style>\r\n"]}]}