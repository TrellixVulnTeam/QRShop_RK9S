,MAAA,IAAA,EAAA,IAPA;AAQA,MAAA,SAAA,EAAA;AARA,KAfA;;AAyBA;AACA;AACA;AACA,IAAA,QAAA,EAAA;AACA,MAAA,OAAA,EAAA,mBAAA;AACA,aAAA,YAAA,GAAA,KAAA,KAAA,GAAA,KAAA,QAAA,GAAA,KAAA,GAAA,IAAA;AACA,OAHA;AAIA,MAAA,IAAA,EAAA,IAJA;AAKA,MAAA,SAAA,EAAA;AALA,KA5BA;;AAmCA;AACA;AACA;AACA;AACA,IAAA,QAAA,EAAA;AACA,MAAA,OAAA,EAAA,mBAAA;AACA,YAAA,MAAA,GAAA,KAAA,QAAA,CAAA,MAAA;;AACA,YAAA,MAAA,GAAA,CAAA,EAAA;AACA,eAAA,SAAA,GACA,MAAA,GAAA,EAAA,GAAA,KAAA,QAAA,CAAA,KAAA,CAAA,CAAA,EAAA,EAAA,CAAA,GAAA,KAAA,QADA;AAEA,SAHA,MAGA,KAAA,SAAA,GAAA,EAAA;;AACA,YAAA,MAAA,IAAA,EAAA,EAAA,KAAA,QAAA,GAAA,CAAA,CAAA,KACA,IAAA,KAAA,QAAA,CAAA,MAAA,GAAA,EAAA,IAAA,CAAA,EACA,KAAA,QAAA,GAAA,KAAA,QAAA,CAAA,MAAA,GAAA,EAAA,CADA,KAGA,KAAA,QAAA,GAAA,IAAA,CAAA,KAAA,CAAA,QAAA,CAAA,KAAA,QAAA,CAAA,MAAA,CAAA,GAAA,EAAA,IAAA,CAAA;AACA,aAAA,KAAA,GAAA,CAAA;AACA,OAbA;AAcA,MAAA,IAAA,EAAA,IAdA;AAeA,MAAA,SAAA,EAAA;AAfA,KAvCA;;AAwDA;AACA;AACA;AACA,IAAA,SAAA,EAAA;AACA,MAAA,OAAA,EAAA,mBAAA;AACA,aAAA,SAAA;AACA,OAHA;AAIA,MAAA,IAAA,EAAA,IAJA;AAKA,MAAA,SAAA,EAAA;AALA;AA3DA;AAzFA,CAAA","sourcesContent":["<template>\r\n  <div class=\"content\">\r\n    <div class=\"container\">\r\n      <div class=\"row\">\r\n        <div class=\"col\">\r\n          <div class=\"btn-group\">\r\n            <button\r\n              class=\"btn btn-default btn-sm dropdown-toggle\"\r\n              type=\"button\"\r\n              data-toggle=\"dropdown\"\r\n              aria-haspopup=\"true\"\r\n              :class=\"[\r\n                { show: isOpen },\r\n                { dropdown: direction === 'down' },\r\n                { dropup: direction === 'up' },\r\n              ]\"\r\n              :aria-expanded=\"isOpen\"\r\n              @click=\"ClickEvent\"\r\n              v-click-outside=\"CloseItem\"\r\n            >\r\n              Icon <i class=\"mdi  mdi-alpha-l-circle\"></i>\r\n            </button>\r\n            <ul\r\n              class=\"dropdown-menu shadow p-3 mb-5 bg-white rounded\"\r\n              :class=\"[{ show: isOpen }]\"\r\n              v-on:click.stop\r\n            >\r\n              <nav aria-label=\"Page navigation example\">\r\n                <ul class=\"pagination d-flex justify-content-between\">\r\n                  <li\r\n                    class=\"page-item\"\r\n                    @click=\"back\"\r\n                    :class=\"[{ disabled: backDisabled }]\"\r\n                  >\r\n                    <a class=\"page-link\" type=\"button\">\r\n                      <span aria-hidden=\"true\">&laquo;</span></a\r\n                    >\r\n                  </li>\r\n                  <li class=\"page-item\">\r\n                    <div class=\"form-group\" style=\"padding:0 0.35em 0 0.35em\">\r\n                      <input\r\n                        type=\"text\"\r\n                        class=\"form-control input-sm\"\r\n                        v-model=\"keyword\"\r\n                        placeholder=\"Search\"\r\n                      />\r\n                    </div>\r\n                  </li>\r\n                  <li\r\n                    class=\"page-item\"\r\n                    @click=\"next\"\r\n                    :class=\"[{ disabled: nextDisabled }]\"\r\n                  >\r\n                    <a class=\"page-link\" type=\"button\"\r\n                      ><span aria-hidden=\"true\">&raquo;</span></a\r\n                    >\r\n                  </li>\r\n                </ul>\r\n              </nav>\r\n              <table class=\"table  table-borderless\">\r\n                <tbody id=\"tbBody\"></tbody>\r\n              </table>\r\n              <div class=\"d-flex justify-content-center\">\r\n                <span class=\"pagination-info\">\r\n                  Showing\r\n                  {{\r\n                    filtered.length > index * 16\r\n                      ? index * 16 - 16\r\n                      : index * 16 - 16\r\n                  }}\r\n                  to\r\n                  {{\r\n                    filtered.length > index * 16 ? index * 16 : filtered.length\r\n                  }}\r\n                  of {{ filtered.length }}\r\n                </span>\r\n              </div>\r\n            </ul>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { ListIcon } from \"./listIcon.js\";\r\nexport default {\r\n  props: {\r\n    direction: { type: String, default: \"down\" },\r\n  },\r\n  data() {\r\n    return {\r\n      icon: ListIcon, //all icon\r\n      index: 0, //index of pagination\r\n      maxIndex: 0, //max of pagination\r\n      nextDisabled: true, //is disable next button\r\n      backDisabled: true, //is disable back button\r\n      filtered: [], //list filter when search. default = all icon\r\n      available: [], //list icon showing\r\n      isOpen: false, //is menu picker show?\r\n      keyword: \"\", //search key\r\n    };\r\n  },\r\n  mounted: function() {\r\n    this.filtered = this.icon; //fist time this.filtered = this.icon(default)\r\n  },\r\n  methods: {\r\n    /**\r\n     * filter condisition\r\n     */\r\n    isLike: function(value) {\r\n      return value.includes(this.keyword);\r\n    },\r\n    /**\r\n     * searching\r\n     */\r\n    search: async function() {\r\n      if (this.keyword != null && this.keyword != \"\")\r\n        this.filtered = this.icon.filter(this.isLike);\r\n      else this.filtered = this.icon;\r\n    },\r\n    /**\r\n     * event when click next pagination\r\n     */\r\n    next: function() {\r\n      this.index += this.maxIndex > this.index ? 1 : 0;\r\n    },\r\n    /**\r\n     * event when click back pagination\r\n     */\r\n    back: function() {\r\n      this.index -= this.index > 1 ? 1 : 0;\r\n    },\r\n    ClickEvent: function() {\r\n      this.isOpen = !this.isOpen;\r\n    },\r\n    CloseItem() {\r\n      this.isOpen = false;\r\n    },\r\n    randomID: function(item) {\r\n      return `ID${item}${parseInt(Math.random() * 99).toString()}`;\r\n    },\r\n    /**\r\n     * init or reload table show icon\r\n     */\r\n    initTable: async function() {\r\n      let length = this.available.length;\r\n      let tb = document.getElementById(\"tbBody\");\r\n      tb.innerHTML = \"\";\r\n      console.log(length);\r\n      let tr = \"\";\r\n      let j = 0;\r\n      let i = 0;\r\n      while (i < 4) {\r\n        if (i == 0) tr = \"<tr>\";\r\n        if (j < length) {\r\n          tr += `<td>\r\n                    <button type=\"button\" class=\"btn btn-outline-primary\">\r\n                        <i class=\"mdi  ${this.available[j]}\"></i>\r\n                      </button>\r\n                </td>`;\r\n        } else {\r\n          tr += \"</tr>\";\r\n          tb.innerHTML += tr;\r\n          break;\r\n        }\r\n        if (i == 3) {\r\n          tr += \"</tr>\";\r\n          tb.innerHTML += tr;\r\n          i = 0;\r\n        } else i++;\r\n        j++;\r\n      }\r\n    },\r\n  },\r\n  watch: {\r\n    /**\r\n     * listen search (change filtered )\r\n     */\r\n    keyword: {\r\n      handler: function() {\r\n        this.search();\r\n      },\r\n      deep: true,\r\n      immediate: false,\r\n    },\r\n    /**\r\n     * calculator list icon show when change index pagination\r\n     * and compare is disable next or back pagination\r\n     */\r\n    index: {\r\n      handler: function() {\r\n        let endLength = this.index * 16;\r\n        this.available = this.filtered.slice(endLength - 16, endLength);\r\n        this.nextDisabled = this.index < this.maxIndex ? false : true;\r\n        this.backDisabled = this.index > 1 ? false : true;\r\n      },\r\n      deep: true,\r\n      immediate: false,\r\n    },\r\n    /**\r\n     * listen when maxIndex pagination then compare is disable next or not?\r\n     */\r\n    maxIndex: {\r\n      handler: function() {\r\n        this.nextDisabled = this.index < this.maxIndex ? false : true;\r\n      },\r\n      deep: true,\r\n      immediate: false,\r\n    },\r\n    /**\r\n     * listen list filter change. If change re calculator maxIndex and set index is 1\r\n     * and showtable are 16 icon fist in list filtered\r\n     */\r\n    filtered: {\r\n      handler: function() {\r\n        let length = this.filtered.length;\r\n        if (length > 0) {\r\n          this.available =\r\n            length > 15 ? this.filtered.slice(0, 16) : this.filtered;\r\n        } else this.available = [];\r\n        if (length <= 16) this.maxIndex = 1;\r\n        else if (this.filtered.length % 16 == 0)\r\n          this.maxIndex = this.filtered.length / 16;\r\n        else\r\n          this.maxIndex = Math.floor(parseInt(this.filtered.length) / 16) + 1;\r\n        this.index = 1;\r\n      },\r\n      deep: true,\r\n      immediate: false,\r\n    },\r\n    /**\r\n     * reload table when list available show change\r\n     */\r\n    available: {\r\n      handler: function() {\r\n        this.initTable();\r\n      },\r\n      deep: true,\r\n      immediate: false,\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style>\r\n.table th,\r\n.table td {\r\n  padding: 0.25rem;\r\n}\r\n.has-search .form-control-feedback {\r\n  right: initial;\r\n  left: 0;\r\n  color: #ccc;\r\n}\r\n\r\n.has-search .form-control {\r\n  padding-right: 12px;\r\n  padding-left: 34px;\r\n}\r\n.disabled {\r\n  pointer-events: none;\r\n}\r\n.pagination-info {\r\n  font-size: 0.75em;\r\n  font-weight: 600;\r\n  color: #4c5761;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/IconPicker"}]}